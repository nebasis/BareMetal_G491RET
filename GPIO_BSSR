#include "stm32g4xx.h"
#include "stm32g491xx.h" //In the course the guy mentioned to use this in the even the board is not reco automatically , go to the g4xx file and see the board or whatever the board is

#define GPIOAEN (1U << 0)
#define PIN5 (1U << 5)
#define LED_PIN PIN5

int main(void)
{
    RCC->AHB2ENR |= GPIOAEN;
    GPIOA->MODER |= (1U << 10);
    GPIOA->MODER &= ~(1U << 11);

    while (1)
    {
        GPIOA->BSRR= LED_PIN;
        for (int i = 0; i < 10000; i++){}

        GPIOA->BSRR = (1U << 21);
        for (int i =0 ; i < 10000;i++){}
    }
}

//Code makes the LED toggle using BSSR regs . 
